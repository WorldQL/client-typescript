{
  "name": "@worldql/client",
  "version": "0.0.7",
  "description": "TypeScript Client for interacting with WorldQL over WebSockets",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "keywords": [
    "worldql",
    "typescript",
    "client",
    "websockets",
    "message broker",
    "spatial database"
  ],
  "scripts": {
    "prepack": "yarn run build",
    "prepublish": "yarn run test",
    "test": "yarn run check && yarn run lint",
    "check": "yarn run compile --noEmit",
    "lint": "xo ./src",
    "clean": "rimraf lib",
    "compile": "tsc",
    "build": "yarn run clean && yarn run compile",
    "dev": "yarn run clean && yarn run compile --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.3",
    "@types/ws": "^8.2.1",
    "rimraf": "^3.0.2",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.2",
    "xo": "^0.47.0"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "flatbuffers": "^2.0.4",
    "isomorphic-ws": "^4.0.1",
    "ws": "^8.3.0"
  }
}
